@using CarInsurance.Models.Table
@model List<Table>

<div class="row row-no-gutters m-3">
    <div class="glyphicon-credit-card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
        <h5 class="col-md-offset-0">Quotes</h5>
    </div>
        <div class="card-body">
            @foreach (var quote in Model)
                        {
                        <div class="row">
                            <div class="col">

                                @quote.FirstName

                            </div>
                            <div class="col">

                                @quote.LastName

                            </div>
                            <div class="col">
                                @quote.EmailAddress
                            </div>
                            <div class="col">
                                @quote.DateOfBirth
                            </div>
                            <div class="col">
                                @quote.CarYear
                            </div>
                            <div class="col">
                                @quote.CarMake
                            </div>
                            <div class="col">
                                @quote.CarModel
                            </div>
                            <div class="col">
                                @quote.DUI
                            </div>
                            <div class="col">
                                @quote.SpeedingTickets
                            </div>
                            <div class="col">
                                @quote.CoverageType
                            </div>
                            <div class="col">
                                @quote.Quote
                            </div>

                                }

                    </div>
        </div>
</div>